{
  "name": "slack-draw-straws",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev": "func host start",
    "test": "echo \"No tests yet...\""
  },
  "author": {
    "name": "Michael Sprague",
    "email": "ms388@cornell.edu"
  },
  "repository": {
    "url": ""
  },
  "engines": {
    "node": "10.x"
  },
  "license": "MIT",
  "dependencies": {
    "@bugsnag/js": "^6.4.3",
    "@slack/web-api": "^5.6.0",
    "snyk": "^1.251.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "1.19.1"
  },
  "babel": {
    "plugins": [],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "import/prefer-default-export": "off",
      "func-names": "off",
      "no-param-reassign": "warn",
      "no-unused-vars": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "plugins": [
      "all"
    ],
    "printWidth": 120
  }
}
