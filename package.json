{
  "name": "slack-draw-straws",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev": "func host start",
    "test": "echo \"No tests yet...\""
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "private": true,
  "repository": {
    "url": "https://github.com/mikesprague/slack-draw-straws"
  },
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 6.x"
  },
  "license": "MIT",
  "dependencies": {
    "@bugsnag/js": "^7.14.1",
    "@slack/web-api": "^5.15.0",
    "snyk": "^1.794.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.6.0"
  },
  "babel": {
    "plugins": [],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "compat",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "import/prefer-default-export": "off",
      "func-names": "off",
      "no-param-reassign": "warn",
      "no-unused-vars": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "plugins": [
      "all"
    ],
    "printWidth": 120
  }
}
